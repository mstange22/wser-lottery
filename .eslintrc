{
    "parser": "babel-eslint",
    "extends": "react-app",
    "rules": {
        "jsx-a11y/no-static-element-interactions": [0, { "handlers": [] }],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/require-default-props": 0,
        "no-underscore-dangle": 0,
        "no-multi-str": 0,
        "comma-dangle": [1, "always-multiline"],
        "semi": 2,
        "no-else-return": 1,
        "no-trailing-spaces": 2,
        "no-multi-spaces": 2,
        "no-multiple-empty-lines": [2, { "max": 1, "maxEOF": 1 }],
        "max-len": ["error", 200],
        "no-console": "off",
        "global-require": "off",
        "no-cond-assign": [0, "except-parents"],
        "no-use-before-define": 0,
        "class-methods-use-this": 0,
        "react/jsx-curly-spacing": 0,
        "react/prop-types": 1,
        "react/no-unused-prop-types": 1,
        "react/forbid-prop-types": 0,
        "no-param-reassign": 0,
        "react/sort-comp": [2, {
            "order": [
                "instance-variables",
                "static-methods",
                "lifecycle",
                "everything-else",
                "render"
            ],
            "groups": {
                "lifecycle": [
                    "displayName",
                    "propTypes",
                    "contextTypes",
                    "childContextTypes",
                    "mixins",
                    "statics",
                    "defaultProps",
                    "constructor",
                    "getDefaultProps",
                    "getInitialState",
                    "state",
                    "getChildContext",
                    "componentWillMount",
                    "componentDidMount",
                    "componentWillReceiveProps",
                    "shouldComponentUpdate",
                    "componentWillUpdate",
                    "componentDidUpdate",
                    "componentWillUnmount"
                ]
            }
        }]
    },
    "globals": {
        "fetch": false,
        "window": false,
        "FormData": false
    },
    "env": {
        "browser": true
    }
}